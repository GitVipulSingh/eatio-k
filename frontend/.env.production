# Production Environment Configuration
# IMPORTANT: Update these URLs with your actual deployment URLs

# Backend API URL (Update with your actual Render backend URL)
VITE_API_URL=https://your-actual-backend-name.onrender.com/api

# Frontend URL (Update with your actual Vercel frontend URL)
VITE_CLIENT_URL=https://your-actual-frontend-name.vercel.app

# App Configuration
VITE_APP_NAME=Eatio

# Payment Gateway (Update with your actual production Razorpay key)
VITE_RAZORPAY_KEY_ID=rzp_live_your_actual_production_key

# Feature Flags
VITE_FEATURE_FLAG_ADMIN_DASHBOARD=true
VITE_FEATURE_FLAG_PAYMENT_GATEWAY=true
VITE_FEATURE_FLAG_RESTAURANT_SIGNUP=true

# Debug Settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=error